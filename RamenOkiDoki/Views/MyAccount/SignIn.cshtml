
@{
    ViewData["Title"] = "Sign In";
}

<h1 class="page-titles">Sign In</h1>

<div class="non-sidebar-page">
    <div class="admin-div">
        <br />
        <p style="margin: 50px 0px 50px; font-weight: bold">User is not Signed In</p>
        <form method="Post">
            <div class="form-group">
                <label>User Name</label>
                <input type="text" id="UserName" />
            </div>
            <div class="form-group">
                <label>Password</label>
                <input type="password" id="Password" />
            </div>

            <a asp-controller="MyAccount" asp-action="SignUserIn" type="submit" class="btn btn-primary submit-order-button">Sign In</a>


        </form>
    </div>
</div>