@model Data.ViewModels.DashboardViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="non-sidebar-page">

    <div class="text-center">
        <h1 class="page-titles">Welcome to Our Table</h1>
    </div>
    <br />
    <br />
    <br />

    <div style="display:inline">
        <Image src="/Images/background.JPG" height="400" width="500" />

        <a href="https://www.facebook.com/okidokiramen/"><i class="fab fa-facebook-square" style="margin:0 10px 0 100px;color: blue;"></i></a>
