@model Data.ViewModels.DashboardViewModel

<form method="post">
    <div class="form-group">
        <label asp-for="BusinessLocation.BusinessName"></label>
        <input type="text" 
               name="@Model.BusinessLocation.BusinessName" 
               value="@Model.BusinessLocation.BusinessName" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessLocation.PhoneNumber"></label>
        <input type="text"
               name="@Model.BusinessLocation.PhoneNumber"
               value="@Model.BusinessLocation.PhoneNumber" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessLocation.Email"></label>
        <input type="text" 
               name="@Model.BusinessLocation.Email" 
               value="@Model.BusinessLocation.Email" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessLocation.StreetAddress"></label>
        <input type="text" 
               name="@Model.BusinessLocation.StreetAddress" 
               value="@Model.BusinessLocation.StreetAddress" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessLocation.City"></label>
        <input type="text"
               name="@Model.BusinessLocation.City"
               value="@Model.BusinessLocation.City" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessLocation.State"></label>
        <input type="text"
               name="@Model.BusinessLocation.State"
               value="@Model.BusinessLocation.State" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessLocation.Zipcode"></label>
        <input type="text"
               name="@Model.BusinessLocation.Zipcode"
               value="@Model.BusinessLocation.Zipcode" />
    </div>
    <button value="DashBoard" type="submit" onclick="location.href='@Url.Action("CloseAddressForm")'" id="addfooditem"><i class="far fa-save"></i>Save</button>
</form>
