@model Data.ViewModels.DashboardViewModel

<form method="post">
    <div class="form-group">
        <label asp-for="BusinessAddress.Phone"></label>
        <input type="text" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessAddress.BusinessName"></label>
        <input type="text" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessAddress.Email"></label>
        <input type="text" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessAddress.Address"></label>
        <input type="text" />
    </div>
    <div class="form-group">
        <label asp-for="BusinessAddress.Website"></label>
        <input type="text" />
    </div>
    <button value="DashBoard" type="submit" onclick="location.href='@Url.Action("DashBoard")'" id="addfooditem"><i class="fas fa-edit"></i>Edit</button>
</form>
